    "# Febonacci Series\n",
    "\n",
    "def febo(n):\n",
    "    a = 0\n",
    "    b = 1\n",
    "    if n <= 1:\n",
    "        return 0\n",
    "    if n == 1:\n",
    "        return 1\n",
    "    if n >= 2:\n",
    "        print(b)\n",
    "        for i in range(1,n):\n",
    "            c = a+b\n",
    "            print(c)\n",
    "            a = b\n",
    "            b = c\n",
    "    "
